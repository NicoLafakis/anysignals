{
  "name": "anysignals",
  "version": "1.0.0",
  "description": "Rate-limited queue service for AnySite API - drips requests at 1 per 10 seconds",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "worker": "node worker.js",
    "dev": "NODE_ENV=development node server.js",
    "dev:worker": "NODE_ENV=development node worker.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "health": "curl -s http://localhost:3456/api/health | jq"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bullmq": "^5.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "",
  "license": "MIT"
}
